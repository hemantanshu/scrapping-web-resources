{
  "name": "scrapping-web-resources",
  "version": "0.0.1",
  "description": "backend business layer for the shopkonnect services",
  "author": "hemantanshu@gmail.com",
  "private": true,
  "license": "MIT",
  "bin": {
    "crun": "dist/main"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "prepare": "npm run build",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config src/config/typeorm.config.ts",
    "m:g": "npm run typeorm migration:generate -- -n",
    "m:r": "npm run typeorm migration:run",
    "m:c": "npm run typeorm migration:create -- -n",
    "m:rev": "npm run typeorm migration:revert",
    "console:dev": "ts-node -r tsconfig-paths/register src/console.ts",
    "command": "npx nestjs-command",
    "console": "node dist/console.js",
    "sonar": "node sonar-project.js"
  },
  "dependencies": {
    "@nestjs/axios": "^0.0.2",
    "@nestjs/bull": "^0.4.1",
    "@nestjs/common": "^8.0.11",
    "@nestjs/core": "^8.0.11",
    "@nestjs/platform-express": "^8.0.11",
    "@nestjs/platform-socket.io": "^8.0.11",
    "@nestjs/terminus": "^8.0.1",
    "@nestjs/typeorm": "^8.0.2",
    "@nestjs/websockets": "^8.0.11",
    "@servicelabsco/nestjs-utility-services": "^1.0.163",
    "@socket.io/redis-adapter": "^7.1.0",
    "@types/randomstring": "^1.1.7",
    "bull": "^3.29.2",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "commander": "^8.2.0",
    "config": "^3.3.6",
    "express-http-context": "^1.2.4",
    "express-rate-limit": "^5.4.1",
    "nest-raven": "^8.0.0",
    "nestjs-command": "^3.1.1",
    "nestjs-console": "^7.0.1",
    "nestjs-redis": "^1.3.3",
    "newrelic": "^8.4.0",
    "node-fetch": "^3.2.3",
    "passport": "^0.5.0",
    "pg": "^8.7.1",
    "randomstring": "^1.2.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.4.0",
    "typeorm": "^0.2.38",
    "typeorm-naming-strategies": "^2.0.0",
    "yargs": "^17.3.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.1.2",
    "@nestjs/schematics": "^8.0.3",
    "@nestjs/testing": "^8.0.11",
    "@types/bull": "^3.15.5",
    "@types/express": "^4.17.13",
    "@types/jest": "27.0.2",
    "@types/node": "^16.10.3",
    "@types/supertest": "^2.0.11",
    "@types/validator": "^13.6.3",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "jest": "^27.2.4",
    "jest-junit": "^13.0.0",
    "prettier": "^2.4.1",
    "sonarqube-scanner": "^2.8.1",
    "supertest": "^6.1.6",
    "ts-jest": "27.0.5",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.2.1",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "testResultsProcessor": "jest-junit"
  }
}